        <externalId>false</externalId>
        <formula>CASE(
    WEEKDAY(DATEVALUE( CheckIn_CMV__Check_in_Date_Time__c )),
    1, &quot;Sunday&quot;,
    2, &quot;Monday&quot;,
    3, &quot;Tuesday&quot;,
    4, &quot;Wednesday&quot;,
    5, &quot;Thursday&quot;,
    6, &quot;Friday&quot;,
    7, &quot;Saturday&quot;,
    &quot;-&quot;
)</formula>
